                },
                "UserId": {
                    "type": "integer"
                },
                "UserEmail": {
                    "type": "string"
                },
                "FromUrl": {
                    "type": "string"
                },
                "NowUrl": {
                    "type": "string"
                },
                "UserIP": {
                    "type": "string"
                },
                "ModelName": {
                    "type": "string"
                },
                "SessionId": {
                    "type": "string"
                }
            }
        }
    }
}